{
  "name": "convert",
  "version": "1.0.0",
  "description": "Script to encode media.",
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild": "0.19.4",
    "eslint": "^8.50.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2"
  },
  "scripts": {
    "test-convert": "npm run bundle-move-test && cd test && node convert.js -a 2 -q 720p -d -o",
    "bundle-move-test": "npm run bundle && npm run copy-outfile-test",
    "copy-outfile-test": "xcopy dist test /Y",
    "bundle": "esbuild src/main.ts --bundle --platform=node --outfile=dist/convert.js",
    "bundle-min": "esbuild src/main.ts --minify --bundle --platform=node --outfile=dist/convert.js"
  },
  "keywords": [
    "boobot"
  ],
  "author": "boooiil",
  "license": "ISC"
}
